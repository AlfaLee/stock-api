{
  "verbose": true,
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "moduleNameMapper": {
    "^@stocks/(.*)$": "<rootDir>/src/stocks/$1",
    "^@utils/(.*)$": "<rootDir>/src/utils/$1",
    "^@interfaces/(.*)$": "<rootDir>/src/interfaces/$1"
  }
}
